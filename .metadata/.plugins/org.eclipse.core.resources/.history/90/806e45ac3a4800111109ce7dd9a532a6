<cfdump var="#questions#" >
<cfdump var="#usernames#">
<cfoutput >
	<ul>
	<cfloop query="users" >	
		<cfloop index="i" from="1" to="#arrayCount(usernames)#">
			<li>#usernames.username#</li>
			<cfloop query="questions">
				<p>Question: #questions.questionId#, Answer: #questions.answer#</p>
			</cfloop>
		</cfloop>
	</cfloop>
	</ul>
</cfoutput>
